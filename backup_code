
  float belief(vector<int> state,  int action){
  float eta=0.0;
  pair<vector<int>, vector<vector<int>>> result=trans_prob(state, action);
  vector<int> next_state=result.first;
  vector<vector<int>> possible_next_states=result.second;
  float denom_bel1=0.0;
  float denom_bel2=0.0;
  for(int i=0; i<possible_next_states.size(); i++){
    denom_bel1+=observation_function(possible_next_states[i], action);
    denom_bel2+=probabilities[i]trans_prob(vector<int> state, int action)


  }

  }
